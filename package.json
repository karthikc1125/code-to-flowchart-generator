{
  "name": "flow",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "electron"
  ],
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:electron": "cd electron && npm run dev",
    "dev:ast2m": "cd ast-to-mermaid && npm run serve",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:electron\" \"npm run dev:ast2m\""
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.34",
    "concurrently": "^8.0.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.4",
    "node-llama-cpp": "^3.8.1",
    "unzipper": "^0.12.3"
  }
}
